<%= form_with model: @room , url: rooms_path , local: true do |f| %>
  <div>
    <%= f.label :施設名 %><br>
    <%= f.text_field :roomname %>
  </div>

  <div>
    <%= f.label :住所 %><br>
    <%= f.text_field :address %>
  </div>

  <div>
    <%= f.label :紹介文 %><br>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.label :料金 %><br>
    <%= f.text_field :price %>
  </div>

  <%= f.submit "登録" %>

<% end %>
