<p>ようこそ</p>

<%= form_with room_path, :method => :get do %>
  <p>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search", name: nil %>
  </p>
<% end %>

<table>
  <% @rooms.each do |room| %>
    <tr>
      <td><%= room.roomname %></td>
	  <td><%= room.address %></td>
	  <td><%= room.description %></td>
	  <td><%= room.price %></td>
    </tr>
  <% end %>
</table>
